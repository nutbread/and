<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="io.nutbread.main" android:versionCode="1" android:versionName="1.0">

	<uses-permission android:name="android.permission.INTERNET" />
